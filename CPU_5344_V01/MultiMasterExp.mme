		<RemoteServer Name="_LMC_X1.ExUnits" Name_DESC="Name" GUID_Svr="{9FE2BF63-FF3B-4C14-B568-13EB87B3E07C}" GUID_Svr_DESC="GUID of the server" DataType="DINT" DataType_DESC="DataType" GUID_Obj="{11177ECE-EE53-434C-A6FE-8AE22A84D121}" GUID_Obj_DESC="GUID of the object" Comment="This value is a part of the transmission ratio. &#13;&#10;InternalPosition [Application Unit] = ExternalPosition [Encoder value] * InternalUnit / ExternalUnit&#13;&#10;The server must be initialized." Comment_DESC="Comment of server"/>
