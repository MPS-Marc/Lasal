		<RemoteServer Name="_LMC_X1.Override" Name_DESC="Name" GUID_Svr="{FF96B1A2-F449-4E94-BD43-CD91AB79A05F}" GUID_Svr_DESC="GUID of the server" DataType="DINT" DataType_DESC="DataType" GUID_Obj="{11177ECE-EE53-434C-A6FE-8AE22A84D121}" GUID_Obj_DESC="GUID of the object" Comment="The override is a percentage reduction of the assigned speed &#13;&#10;for the actual motion command. (1000 = 100.0%)." Comment_DESC="Comment of server"/>
